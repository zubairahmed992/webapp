<h1>Merch creation</h1>

<form action="{{ path('merch_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('merch') }}">
            Back to the list
        </a>
    </li>
</ul>



 <table>
    <tbody>
    <tr>
        <th>Id</th>
        <th>Name</th>
        
        
    </tr>

{% for b in brands %}
    <tr>
        <td>{{b.Id}}</td>
        <td>{{b.Name}}</td>
        
    </tr>
    </tbody>
{% endfor %}
</table>
