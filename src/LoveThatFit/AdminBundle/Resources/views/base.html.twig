       <script type="text/javascript">
	jQuery(document).ready(function() {
	  	jQuery('#rootwizard').bootstrapWizard({onTabShow: function(tab, navigation, index) {
			var $total = navigation.find('li').length;
			var $current = index+1;
			var $percent = ($current/$total) * 100;
			jQuery('#rootwizard').find('.bar').css({width:$percent+'%'});
		}});
        //for user
        //$("#keyword").keyup(function() {
        $('#keyword').on('keyup paste', function() {
            $("#keyword").autocomplete({
                minLength: 2,
                //source: "autocomplete_user_search_result",
                source:function (request, response) {
                $.ajax({
                    type: "POST",
                    url:"../autocomplete_user/autocomplete_user_search_result",
                    data: request,
                    success: response,
                    dataType: 'json'
                });
            },
                focus: function( event, ui ) {
                    $( "#keyword" ).val( ui.item.email);
                    return false;
                },
                select: function( event, ui ) {
                    $("#keyword").val(ui.item.email);
                    $("#user_id").val(ui.item.id);
                    $('#user_User option[value='+ui.item.id+']').attr('selected','selected');
                    $('#algorithm_User option[value='+ui.item.id+']').attr('selected','selected');
                    hit_service();
                    var url= '../user/'+ ui.item.id +'/edit';
                    $('#user_edit_link').attr("href", url);
                    return false;
                }
            })
                .autocomplete( "instance" )._renderItem = function( ul, item ) {
                return $( "<li>" )
                        .append( "<a>" + item.email + "</a>" )
                        .appendTo( ul );
            };
        });
        //for products
        $("#product_keyword").keyup(function() {
            $("#product_keyword").autocomplete({
                minLength: 2,
                source:function (request, response) {
                    $.ajax({
                        type: "POST",
                        url:"../autocomplete_product/autocomplete_product_search_result",
                        data: request,
                        success: response,
                        dataType: 'json'
                    });
                },
                focus: function( event, ui ) {
                    $( "#product_keyword" ).val( ui.item.name);
                    return false;
                },
                select: function( event, ui ) {
                    $("#product_keyword").val( ui.item.name);
                    $("#product_id").val( ui.item.id );
                    $('#algorithm_Product option[value='+ui.item.id+']').attr('selected','selected');
                    var url= '../productdetail/'+ui.item.id+'';
                    $('#product_edit_link').attr("href", url);
                    return false;
                }
            })
                    .autocomplete( "instance" )._renderItem = function( ul, item ) {
                return $( "<li>" )
                        .append( "<a>" + item.name + "</a>" )
                        .appendTo( ul );
            };
        });
        //for import page path difference so another function written
        //for products
        $("#product_keyword_import").keyup(function() {
            $("#product_keyword_import").autocomplete({
                minLength: 2,
                source:function (request, response) {
                    $.ajax({
                        type: "POST",
                        url:"../../autocomplete_product/autocomplete_product_search_result",
                        data: request,
                        success: response,
                        dataType: 'json'
                    });
                },
                focus: function( event, ui ) {
                    $( "#product_keyword_import" ).val( ui.item.name);
                    return false;
                },
                select: function( event, ui ) {
                    $("#product_keyword_import").val( ui.item.name);
                    $("#product_id").val( ui.item.id );
                    $('#form_products option[value='+ui.item.id+']').attr('selected','selected');
                    return false;
                }
            })
                    .autocomplete( "instance" )._renderItem = function( ul, item ) {
                return $( "<li>" )
                        .append( "<a>" + item.name + "</a>" )
                        .appendTo( ul );
            };
        });
        //end
        //for services page specifically for users drop down
        //for user
        $("#keyword_user").keyup(function() {
            $("#keyword_user").autocomplete({
                minLength: 2,
                //source: "autocomplete_user_search_result",
                source:function (request, response) {
                    $.ajax({
                        type: "POST",
                        url:"../admin/autocomplete_user/autocomplete_user_search_result",
                        data: request,
                        success: response,
                        dataType: 'json'
                    });
                },
                focus: function( event, ui ) {
                    $("#keyword_user").val( ui.item.email);
                    return false;
                },
                select: function( event, ui ) {
                    $("#keyword_user").val(ui.item.email);
                    $("#user_id").val(ui.item.id);
                    //alert(ui.item.email);
                    //$('#sel_user option[value='+ui.item.id+']').attr('selected','selected');
                    //$('#sel_user').text(ui.item.email).attr('selected','selected');
                    //$('#sel_user').text($('option:selected', ui.item.email));
                    //.attr('selected','selected');
                    $("#sel_user option:contains(" + ui.item.email + ")").attr('selected', 'selected');
                    change_user_detail();
                    replace_user_data();
                    return false;
                }
            })
                    .autocomplete( "instance" )._renderItem = function( ul, item ) {
                return $( "<li>" )
                        .append( "<a>" + item.email + "</a>" )
                        .appendTo( ul );
            };
        });
        //end
        function hit_service(){}
	});	
	</script>

