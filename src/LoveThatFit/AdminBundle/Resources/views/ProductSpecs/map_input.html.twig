{% extends 'LoveThatFitAdminBundle::base.html.twig' %}
{% block breadcrumb %}
  <li><a href="{{ path('admin_products') }}" >Products Specification</a> </li>   
{% endblock %}

{% block body %}
  
  
   <h1 style="float:left;">Product Brand CSV Format Configure</h1>
    <div class="btn_wrapper"><a href="{{ path('admin_products') }}" class="btn">back to list</a></div>
    <div class="clearfix"></div>

    <div id="detail_wrapper">
        <div id="product_content">
            {#{{ dump(brandNames) }}#}
            <form action="{{ path('admin_product_specs_mapping_save') }}" method="post" >
