                    <div class="clearfix"></div>
                    <div id="product_content">
                        <div id="pro_detail">
                            <div class="edt_container">
                                <div class="row-fluid show-grid" id="brand_1">
                                    <div class="span3">Please Select Brand</div>
                                    <div class="span3">
                                        <select name="brand_name">
                                            {% for b in brands %}
                                                <option value="{{ b.name }}"> {{ b.name }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
                                </div>
                                     <div class="row-fluid show-grid">
                                    <div class="span3">Please Select Gender</div>
                                    <div class="span3">
                                        <select name="brand_name">
                                                <option value="m"> Man </option>                                            
                                                <option value="f"> Woman </option>                                            
                                        </select>
                                    </div>
                                </div>
                                <div class="row-fluid show-grid" id="brand_1">
                                    <div class="span3">Clothing Type</div>
                                    <div class="span3">
                                        <select name="clothing_type">
                                            {% for k, v in clothing_types.woman %}
                                                <option value="{{ k }}"> {{ v }}</option>
                                            {% endfor %}
                                        </select>
                                    </div>
                                </div>
                                <div class="row-fluid show-grid">
                                    <div class="span3">Description</div>
                                    <div class="span3">
