      var admin_algorithm_product= "../admin/algorithm/product/"+ product_id;         
      jQuery.ajax({
       type: "POST",
       url: admin_algorithm_product,              
       success: function(response){                            
           $('#productAjax').html(response);
       }
     });   
 })
});           
        </script>
