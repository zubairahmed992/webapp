    // Retailer Section 
 
 
 $("#product_Retailer").change(function(){
     var retailer_id=$('#product_Retailer').val();
     $.ajax({
        type:'POST',
        url: $('#retailer_url').text(),
        data: {retailer_id: retailer_id},  
        success: function(data) {
        var brand_list = JSON.parse(data);
        $('#product_Brand').empty();
        length_obj=Object.keys(brand_list).length;
        if( length_obj>0){
        for(i=0;i<length_obj;i++)
        {   
         $('<option>').val(brand_list[i]['brand_id']).text(brand_list[i]['brand_name']).appendTo('#product_Brand');    
        }
      } 
       else 
           {
             $('<option>').val('').text('Unavailable').appendTo('#product_Brand');       
           }
               
                
    }})

});
 
