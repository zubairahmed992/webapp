    ////////Edit for fit type pirorty////////////////
    {% autoescape false %}
   {%if(fit_priority)%}
    var json = {{fit_priority}};
    {%else%}
    var json=null;  
    {%endif%}
   if(json!=null)  {       
     length_obj=json.length;
     Object.size = function(obj) {
     var size = 0, key;
     for (key in obj) {
     if (obj.hasOwnProperty(key)) size++;
     }
     return size;
 };
 var size = Object.size(json);
 s="";
 s+='<label>Fit Pirority:</label>';
 for(var key in json) {
     var count=1;
    s+= '<label>'+key+'</label>';
    s+=' <input type="text" class="required" id="fit_pirority'+count+'"  name="fit_pirority['+key+']" value="'+json[key]+'">'; 
   count++;
}
           
 document.getElementById("fit_priority").innerHTML=s; 
 }else{
var clothing_type=$("#product_ClothingType").val();
var gender_type=$("#product_gender").val();
$.ajax({
        type:'POST',
        url: $('#fit_pirority_url').text(),
        data: {clothing_type: clothing_type,gender:gender_type},  
        success: function(data) {
       
           $('#fit_priority').show();
           var s="";
           s+= '<label>Fit Pirority:</label>'; 
           var result = $.parseJSON(data);
           $.each(result['fitting_priority'], function(k, v) {
           s+= '<label>'+v+'</label>';  
           s+= '<input class="required" type="text" name="fit_pirority['+v+']">'; 
           });
          document.getElementById("fit_priority").innerHTML=s;
        }})




}
