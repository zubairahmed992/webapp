<form  onsubmit="return validate();" action="{{ path('admin_product_raw_update', { 'id': entity.id }) }}" method="post" {{ form_enctype(form) }}>
 {{ form_errors(form) }}

        <div id="detail_wrapper">      
            <div id="p_hd">Product Detail</div>

            <div class="row-fluid show-grid">
                <div class="span3">Fit Priority</div>
                <div class="span3">{{ form_widget(form.fit_priority) }}{{ form_errors(form.fit_priority) }}</div>
            </div>
            <div class="row-fluid show-grid">
                <div class="span3">&nbsp;</div>

                <div class="span3">
				{{ form_rest(form) }}
                        <div style="margin:0px 0px 0px 0px;"> <input type="submit" value="Update" class="btn  btn-primary"/>                                     

                        </div>                                    
                    </div>
                </div>
            </div>



        </form> 



