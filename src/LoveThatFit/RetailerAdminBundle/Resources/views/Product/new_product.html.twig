{% extends 'LoveThatFitRetailerAdminBundle::base.html.twig' %}
  
{% block breadcrumb %}
 <li class="active">Retailer Dashboard </li>

         
          
{% endblock %}
{% block h1 %}Product{% endblock %}

{% block title %}Product{% endblock %}


{% block body %}
<script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js">
</script>

<script>
$(document).ready(function(){
    
 $('#optional_elements').hide();
 $('#product_ClothingType').empty();
 $("#product_gender").change(function(){
     var gender=$('#product_gender').val();
     // Clothing Type For Female
     $.ajax({
        type:'POST',
        url: $('#url').text(),
        data: {gender: gender},  
        success: function(data) {
        $('#product_ClothingType').empty();
      var clothing_type = JSON.parse(data);
      length_obj=Object.keys(clothing_type).length;
      if( length_obj>0)
       {    
        for(i=0;i<length_obj;i++)
        {   
            
           
         $('<option>').val(clothing_type[i]['id']).text(clothing_type[i]['name']).appendTo('#product_ClothingType');    
        }
      } 
       else 
           {
             $('<option>').val('').text('Unavailable').appendTo('#product_ClothingType');       
           }
           
           
                
    }})

});
 $("#product_ClothingType").change(function(){
 var gender=$('#product_gender').val();
 if(gender=='F'){
  
  $('#product_styling_type').empty();
  $('#product_hem_length').empty();
  $('#product_neckline').empty();
  $('#product_sleeve_styling').empty();
  $('#product_rise').empty();
womenClothingType();
}
else{
  $('#product_styling_type').empty();
  $('#product_hem_length').empty();
  $('#product_neckline').empty();
  $('#product_sleeve_styling').empty();
  $('#product_rise').empty();
manClothingType();
}

// Sending ajax request for fit pirority  and create dynamic elements

var clothing_type=$("#product_ClothingType").val();
var gender_type=$("#product_gender").val();
 $.ajax({
        type:'POST',
        url: $('#fit_pirority_url').text(),
        data: {clothing_type: clothing_type,gender:gender_type},  
        success: function(data) {
           
           //fit_priority 
        //fabric_content
      var clothing_type_attribute = JSON.parse(data);
      length_obj=Object.keys(clothing_type_attribute).length;
     
      if( length_obj>0)
       {  
           $('#fit_priority').show();
           var s="";
           s+= '<lable>Fit Pirority:</label>'; 
           var result = $.parseJSON(data);
            $.each(result['fitting_priority'], function(k, v) {
             s+= '<lable>'+v+'</lable>';  
             s+= '<input type="text" name="fit_pirority['+v+']">'; 
              
            });
            
           //For Fabric Content 
           var fabric_content="";
           $('#fabric_content').show();
            fabric_content+= '<lable>Fabric Content:</label>'; 
           var result = $.parseJSON(data);
            $.each(result['fabric_content'], function(k, v) {
             fabric_content+= '<lable>'+v+'</lable>';  
             fabric_content+= '<input type="text" name="fit_pirority['+v+']">'; 
              
            });

        //$('#product_fit_priority').val();
         document.getElementById("fit_priority").innerHTML=s;
         document.getElementById("fabric_content").innerHTML=fabric_content;
         
      } 
     
           
           
        
    }})



});





})
function strtolower (str) {
